<% provide(:title) %>

<h1>Welcome to the Our Walls</h1>

<% if signed_in? %>
  <p>Sign out</p>
<% else %>
  <%= link_to "Sign up now!", signup_path,
              class: "btn btn-large btn-primary" %>
  <%= link_to "Sign in", signin_path,
              class: "btn btn-large btn-primary" %>
<% end %>
